<%= form_for( @photo, :url => album_photos_path(album_id: @album.id), :html => { :multipart => true } ) do |f| %>
<%= render 'shared/error_messages', object: f.object %>

<div class="field">
  <%= f.label :name, "Name" %>
  <%= f.text_field :name %>
  <%= f.label :description %>
  <%= f.text_area :description, placeholder: "Type photo description..." %>
  
<% end %>